trigger(\"change\");\n    }\n})\n\n// 办结时限默认为18点；自定义脚本(如果用户手动选择了时间为00，也会重置为18)：\n$(\"[name='deadline']\").on(\"change\",function(e){\n  var val = $(this).val();\n  if(val){\n    var dl = new Date($(this).val());\n    if(dl.getHours() == 0 && dl.getMinutes() == 0){\n      $(this).val($.format.date(dl, \"yyyy-MM-dd\") + \" 18:00\")\n    }\n  }\n})\n\n//// 发送事件\n$(\".instance-form\").on('instance-before-submit', function(e) {\n    //判断归档是否为空\n    needRecord(e);\n});\n//   needRecord:是否需要上传办理结果\nfunction needRecord(e){\n    if($('select[name=\"FONDSID\"]').length){\n        var record_need = $('input:checkbox[name=\"record_need\"]:checked').val();\n        var FONDSID = $('select[name=\"FONDSID\"]').val();\n        if(record_need&&!FONDSID){\n            swal({title:\"请选择归档单位\", type:\"error\", confirmButtonText: \"确认\"});\n            e.preventDefault();\n        } \n    }\n}\n\n\n$(\"#instance_cc_modal\").on('before-confirm', function(e) {\n    // if ：传阅对象中包含了“总法律顾问”\n    if  ($(\"[name='cc_users']\", $(\"#instanceCCForm\")).val().search('马喜平') != -1 ) {\n        // if ：传阅对象 大于等于1人\n        if ($(\"[name='cc_users']\", $(\"#instanceCCForm\")).val().search(',') != -1) {\n            // 弹出警告框、提示总法律顾问与其他人员的选项应不同\n            swal({title:\"传阅对象包含了总法律顾问，应与其他人员的选项不同。请单选总法律顾问，或只选择其他人员。\", type:\"error\", confirmButtonText: \"确认\"});\n            e.preventDefault();\n        // else  : 传阅对象=1、且为总法律顾问\n        } else {\n            //  只认为是 勾选了 总法律顾问意见\n            $(\"[value='总法律顾问']\", $(\"#instanceCCForm\")).prop(\"checked\",true);\n            $(\"[value='主办部室']\", $(\"#instanceCCForm\")).prop(\"checked\",false);\n            $(\"[value='其他部室']\", $(\"#instanceCCForm\")).prop(\"checked\",false);\n        }\n    // else  : 即不包含总法律顾问\n    } else { \n        // 都认为没有勾选了 总法律顾问意见\n        $(\"[value='总法律顾问']\", $(\"#instanceCCForm\")).prop(\"checked\",false);\n        // 如果3个都没勾，认为是勾了后2个\n        if ( !$(\"[value='主办部室']\", $(\"#instanceCCForm\")).prop(\"checked\")  &&  !$(\"[value='其他部室']\", $(\"#instanceCCForm\")).prop(\"checked\")) {\n            $(\"[value='主办部室']\", $(\"#instanceCCForm\")).prop(\"checked\",true);\n            $(\"[value='其他部室']\", $(\"#instanceCCForm\")).prop(\"checked\",true);\n        }\n    }\n \n })","modified":"2023-12-22T07:35:50.061Z","distribute_to_self":true,"instance_style":"table","historys":[],"api_name":"asking","auto_remind":false,"sort_no":null,"timeout_auto_submit":false,"allow_select_step":false,"category":"6582443694642305cca0040f_dc3ffc8350b64b25b9628438","import":true,"modified_by":"6582443294642305cca0040d","instances":null,"created_by":"6582443294642305cca0040d","company_id":"6582443694642305cca0040f","company_ids":["6582443694642305cca0040f"],"owner":"6582443294642305cca0040d","object_workflows":[{"_id":"dwhRByqv8XvwHzGZF","sync_type":"every_step","sync_direction":"ins_to_obj","object_name":"asking","name":"公司请示报告","flow_id":"10b4aa0fc5243990708e51c4","sync_attachment":"lastest","field_map_back":[{"object_field":"name","workflow_field":"文件标题"},{"object_field":"organization","workflow_field":"请示单位"},{"object_field":"date","workflow_field":"拟稿日期"},{"object_field":"manager","workflow_field":"负责人签字"},{"object_field":"priority","workflow_field":"priority"},{"object_field":"deadline","workflow_field":"deadline"},{"object_field":"bljg","workflow_field":"需上传办理结果"},{"object_field":"ssbm","workflow_field":"所属部门"}]}]}]}